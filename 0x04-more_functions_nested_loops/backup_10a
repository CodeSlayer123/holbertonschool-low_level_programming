#include "main.h"
/**
 * print_triangle - prints triangle
 *
 * Return: Always 0.
 */

void print_triangle(int size)
{

  int row = size;
  int inner;
  int outer;
 

  for (outer = 0; outer < size; outer++)
    {
      int spaces = row - outer;

      for (inner = 0; inner <= spaces; inner++) 
	{
	  _putchar(' ');
	}
      _putchar('#');
      _putchar('\n');
    }
}

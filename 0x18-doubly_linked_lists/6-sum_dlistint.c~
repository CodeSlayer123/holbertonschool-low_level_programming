#include <stdio.h>
#include <stdlib.h>
#include "lists.h"
#include <string.h>

/**
 * sum_dlistint - find sum of all data (n) of a listint_t linked list
 * @head: head
 * Return: returns the node at nth index
 */

int sum_dlistint(listint_t *head)
{
	dlistint_t *tmp = head;
	int sum;

	sum = 0;
	if (head == NULL)
	{
		return (0);
	}
	while (tmp != NULL)
	{
		sum += tmp->n;
		tmp = tmp->next;
	}
	return (sum);
}
